	public void say_nick(String nick){
		if("fool".equals(nick)){
			return;
		}
		System.out.println("나의 별명은 "+nick+" 입니다.");
	}
	
	
	public int vartest(int a){
		a++;
		return a;
	}
	
	
	public static void main(String[] args) {

		Test test = new Test();
		int a =1;
		a= test.vartest(a);
		a= test.vartest(a);
		System.out.println(a);
		
		
	}

	
	

