package test;

import java.util.*;

public class Test12 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		while(true){
			System.out.print("숫자입력>>");
			int input = sc.nextInt();
			int cnt=0;
			for(int i=1;i<=input;i++){
				
				if(input%i==0){
					cnt++;
				}
				
				if(cnt ==2){
					System.out.println(input);
					break;
				}
			}
			
			
		}
		
		
		
	}

}
