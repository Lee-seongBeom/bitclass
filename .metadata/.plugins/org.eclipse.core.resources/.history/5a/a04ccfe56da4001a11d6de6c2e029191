package com.bit.day20;

import java.util.*;

public class Ex02 {

	public static void main(String[] args) {
		Vector vec01 = new Vector();
		System.out.println(vec01.capacity());
		for(int i=0;i<21;i++){
			vec01.addElement(i);
		}
		
		
		System.out.println(vec01.capacity());
		ArrayList list = new ArrayList();
		
		list.add(1);
		list.add(2);
		list.add(3);
		list.add(4);
		list.add(4);
		Vector vec02 = new Vector(list);
		System.out.println(vec02.capacity());		
//		Enumeration em = vec02.elements();
//		while(em.hasMoreElements()){
//			System.out.println(em.nextElement());
//		}
//		
		Vector vec03 = new Vector(3);
		System.out.println(vec03.capacity());
		
		for(int i=0;i<50;i++){
			vec03.addElement(i);
		}
		
		System.out.println(vec03.capacity());
		
		
		
		
	}

}










