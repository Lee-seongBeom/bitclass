package com.bit.day20;

class Lec10{
	int su;
	String msg;
	
	void setSu(int su){
		this.su=su;
	}
	
	void setMsg(String msg){
		this.msg=msg;
	}
	
	int getSu(){
		return su;
	}
	
	String getMsg(){
		return msg;
	}
}

public class Ex10 {

	public static void main(String[] args) {
		Lec10 lec =new Lec10();
		lec.setSu(1234);
		lec.setMsg("hello");
		
		System.out.println(lec.getSu());
	}

}
