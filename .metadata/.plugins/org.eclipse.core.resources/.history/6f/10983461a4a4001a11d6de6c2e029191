package com.bit.day20;
class Lec13{
	
	void func(){
		System.out.println("기능사용");
	}
}

class Template{
	static Box<BaseBall> newInstance(){
		Box<BaseBall> lec=new Box<>();
		lec.push(new BaseBall());
		return lec;
	}
}
public class Ex13 {

	public static void main(String[] args) {
		Box<BaseBall> lec =Template.newInstance();
		lec.pull().play();
	}

}
